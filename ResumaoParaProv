	// laço de repetição
      
//while
  while(variavel = 0){
  System.out.println("Igual a 0")
  }

//for
  for(int variavel = 0; variavel = 0; variavel++){
  System.out.println("Igual a 0")
  }
	// condição
      
//if
  if(variavel = 0){
  System.out.println("Igual a 0")
  }
	// String
      
//criar uma String
  String Frase = "Frase dentro da String"
  
//tamanho de String
  Varivel = Frase.lenght();
  
//site com os codigos de Strings
https://www.devmedia.com.br/trabalhando-com-string-string-em-java/21737

	// Vetores

//declaração de vetores
  int[] vetor;
  //O int pode ser alterado por outras variaveis como double e string.
  
//tamanho do vetor
  Varivel = vetor.lenght();

//site de vetor
  https://www.devmedia.com.br/vetores-em-java/21449

	// Função
      
//Como se escreve
  static int função(int variavel1, int variavel2){
      return variavel1+variavel2;
  }
  //O int pode ser alterado por outras variaveis como double e string.


